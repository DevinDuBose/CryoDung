<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
      prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label layoutX="250.0" layoutY="20" prefWidth="200.0" text="CryoDung">
            <font>
                <Font name="Arial Black" size="24.0"/>
            </font>
        </Label>
        <ImageView fx:id="logo"
                   disable="false"
                   fitHeight="50.0" fitWidth="50.0"
                   layoutX="291.0" layoutY="55.0"
                   pickOnBounds="true"
                   preserveRatio="true"/>

        <Label layoutX="225.0" layoutY="110.0" text="Complexity:"/>
        <ComboBox fx:id="complexity" layoutX="357.0" layoutY="110.0"/>
        <Label layoutX="225.0" layoutY="150.0" text="Floor Size:"/>
        <ComboBox fx:id="size" layoutX="357.0" layoutY="150.0"/>
        <Label layoutX="225.0" layoutY="190.0" text="Train Skills?"/>
        <CheckBox fx:id="trainSkills" layoutX="357.0" layoutY="190.0"/>
        <Label layoutX="225.0" layoutY="230.0" text="Starting Room:"/>
        <Label fx:id="startRoomLabel" layoutX="357.0" layoutY="230.0" text="N/A"/>
        <Label layoutX="225.0" layoutY="270.0" text="Current Room:"/>
        <Label fx:id="currentRoomLabel" layoutX="357.0" layoutY="270.0" text="N/A"/>
        <Label layoutX="225.0" layoutY="310.0" text="Dungeons Entered:"/>
        <Label fx:id="dungeonsEnteredLabel" layoutX="357.0" layoutY="310.0" text="N/A"/>
        <Label layoutX="225.0" layoutY="350.0" text="Dungeons Finished:"/>
        <Label fx:id="dungeonsFinishedLabel" layoutX="357.0" layoutY="350.0" text="N/A"/>

    </children>
</Pane>